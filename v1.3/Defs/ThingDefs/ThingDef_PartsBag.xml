<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef ParentName="ResourceBase">
    <defName>PartsBag</defName>
    <label>Parts Bag</label>
    <description>A small bag filled with components from a dismantled item.</description>
    <thingClass>Rimvention.Thing_PartsBag</thingClass>
    <drawGUIOverlay>true</drawGUIOverlay>
    <graphicData>
      <texPath>Things/Items/Test/FinishedExp</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <statBases>
      <Mass>0.5</Mass>
      <MarketValue>50</MarketValue>
    </statBases>
    <thingCategories>
      <li>Items</li>
    </thingCategories>
    <comps>
      <li Class="Rimvention.PartsBagCompProperties" />
    </comps>
    <tradeNeverStack>true</tradeNeverStack>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <stackLimit>1</stackLimit>
    <useHitPoints>true</useHitPoints>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
  </ThingDef>

</Defs>