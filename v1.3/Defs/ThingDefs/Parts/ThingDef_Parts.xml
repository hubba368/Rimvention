<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Base -->

<ThingDef ParentName="ResourceBase" Name="AugmentPartBase" Abstract="True">
    <statBases>
      <Mass>0.5</Mass>
      <MarketValue>50</MarketValue>
    </statBases>
    <thingCategories>
      <li>AugmentParts</li>
    </thingCategories>
    <tradeNeverStack>true</tradeNeverStack>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <stackLimit>50</stackLimit>
    <useHitPoints>true</useHitPoints>
    <tradeability>Sellable</tradeability>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
  </ThingDef>

<!-- Common Parts -->

<ThingDef ParentName="AugmentPartBase">
    <defName>SimplePart</defName>
    <label>Simple Part</label>
    <description>A simple part acquired from disassembling something.</description>
    <drawGUIOverlay>true</drawGUIOverlay>
    <graphicData>
      <texPath>Things/Items/Test/FinishedExp</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
  </ThingDef>


